# Version checker

set(PACKAGE_VERSION @ant_engine_VERSION@)

if("${PACKAGE_FIND_VERSION_MAJOR}" EQUAL "@ant_engine_VERSION_MAJOR@")
	if("${PACKAGE_FIND_VERSION_MINOR}" EQUAL "@ant_engine_VERSION_MINOR@")
		set(PACKAGE_VERSION_EXACT TRUE)
	elseif("${PACKAGE_FIND_VERSION_MINOR}" LESS "@ant_engine_VERSION_MINOR@")
		set(PACKAGE_VERSION_COMPATIBLE TRUE)
	else()
		set(PACKAGE_VERSION_UNSUITABLE TRUE)
	endif()
else()
	set(PACKAGE_VERSION_UNSUITABLE TRUE)
endif()
